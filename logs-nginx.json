{
  "template": {
    "settings": {
      "index": {
        "default_pipeline": "enrich-nginx",
        "number_of_replicas": 0
      }
    },
    "mappings": {
      "properties": {
        "data_stream": {
          "type": "object",
          "properties": {
            "dataset": {
              "type": "keyword"
            }
          }
        },
        "event": {
          "type": "object",
          "properties": {
            "category": {
              "type": "keyword"
            },
            "dataset": {
              "type": "keyword"
            },
            "ingested": {
              "type": "date"
            },
            "kind": {
              "type": "keyword"
            },
            "module": {
              "type": "keyword"
            },
            "outcome": {
              "type": "keyword"
            }
          }
        },
        "http": {
          "type": "object",
          "properties": {
            "request": {
              "type": "object",
              "properties": {
                "method": {
                  "type": "keyword"
                }
              }
            },
            "response": {
              "type": "object",
              "properties": {
                "body": {
                  "type": "object",
                  "properties": {
                    "bytes": {
                      "type": "long"
                    }
                  }
                },
                "status_code": {
                  "type": "long"
                }
              }
            },
            "version": {
              "type": "keyword"
            }
          }
        },
        "log": {
          "type": "object",
          "properties": {
            "file": {
              "type": "object",
              "properties": {
                "path": {
                  "type": "keyword"
                }
              }
            }
          }
        },
        "source": {
          "type": "object",
          "properties": {
            "geo": {
              "type": "object",
              "properties": {
                "continent_name": {
                  "type": "keyword"
                },
                "country_iso_code": {
                  "type": "keyword"
                },
                "country_name": {
                  "type": "keyword"
                },
                "location": {
                  "type": "geo_point"
                }
              }
            },
            "ip": {
              "type": "ip"
            }
          }
        },
        "user_agent": {
          "type": "object",
          "properties": {
            "device": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "keyword"
                },
                "type": {
                  "type": "keyword"
                }
              }
            },
            "name": {
              "type": "keyword"
            },
            "original": {
              "type": "keyword"
            },
            "os": {
              "type": "object",
              "properties": {
                "full": {
                  "type": "keyword"
                },
                "name": {
                  "type": "keyword"
                },
                "version": {
                  "type": "keyword"
                }
              }
            },
            "version ": {
              "type": "keyword"
            }
          }
        }
      }
    }
  },
  "index_patterns": [
    "logs-nginx*"
  ],
  "composed_of": [
    "logs@settings",
    "ecs@mappings"
  ],
  "ignore_missing_component_templates": [],
  "allow_auto_create": true
}
